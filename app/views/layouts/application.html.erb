<!DOCTYPE html>
<html>
<head>
  <title>Mag</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

        <% if current_user %>
          Logged in as <%= current_user.title %> <%= current_user.name %> <%= current_user.surname %>.
          <%= link_to "Log Out", logout_path %>
        <% else %>
          <%= link_to "Sign Up", signup_path %> or
          <%= link_to "Log In", login_path %>
        <% end %>
<br />
<%= link_to "Subject", subjects_path %> <%= link_to "All Groups", groups_path %> <%= link_to "All Students", students_path %>
<br />
     <% flash.each do |name, msg| %>
        <%= content_tag :div, msg, id: "flash_#{name}" %>
      <% end %>
<%= yield %>
<%= debug(params) if Rails.env.development? %>
</body>
</html>
